{
  "name": "@DaniOlivares/cognifit-sdk",
  "version": "1.0.19",
  "description": "Connect your app with CogniFit",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/*.ts\" \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/DaniOlivares"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniOlivares/cognifit-sdk.git",
    "directory": "@DaniOlivares/cognifit-sdk"
  },
  "keywords": [
    "CogniFit",
    "Cognitive",
    "Assessments",
    "Training",
    "Games"
  ],
  "author": "CogniFit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DaniOlivares/cognifit-sdk/issues"
  },
  "homepage": "https://github.com/DaniOlivares/cognifit-sdk#readme",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "lib/**/*"
  ]
}
